{
  "name": "next-web-template",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && yarn gather",
    "start": "node ./build/server.js",
    "lint": "next lint",
    "postinstall": "husky install && playwright install --with-deps",
    "clean": "yarn node ./scripts/clean.js",
    "test:local": "playwright test --ui",
    "test:ci": "playwright test",
    "gather": "yarn node ./scripts/gather.js"
  },
  "lint-staged": {
    ".{js,jsx,ts,tsx}": [
      "yarn eslint --fix"
    ],
    "*": [
      "yarn cspell --no-must-find-files",
      "yarn prettier --ignore-unknown --write"
    ]
  },
  "packageManager": "yarn@3.6.1",
  "dependencies": {
    "@fluentui/react-components": "^9.26.2",
    "@fluentui/react-icons": "^2.0.207",
    "@formatjs/intl-localematcher": "^0.4.0",
    "i18next": "^23.2.11",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-resources-to-backend": "^1.1.4",
    "negotiator": "^0.6.3",
    "next": "^13.4.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.2",
    "scheduler": "^0.19.0 || ^0.20.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.36.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/negotiator": "^0.6.1",
    "@types/node": "^20.4.4",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "conventional-changelog-conventionalcommits": "^6.1.0",
    "cspell": "^6.31.2",
    "eslint": "^8.45.0",
    "eslint-config-next": "^13.4.12",
    "husky": "^8.0.3",
    "hygen": "^6.2.11",
    "lint-staged": "^13.2.3",
    "pinst": "^3.0.0",
    "prettier": "<3.0.0",
    "prettier-plugin-jsdoc": "<1.0.0",
    "semantic-release": "^21.0.7",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
